{
  "name": "@department-of-veterans-affairs/react-jsonschema-form",
  "version": "1.2.5",
  "description": "A simple React component capable of building HTML forms out of a JSON schema.",
  "scripts": {
    "build:readme": "toctoc README.md -w",
    "build:lib": "rimraf lib && cross-env NODE_ENV=production babel -d lib/ src/",
    "build:dist": "rimraf dist && cross-env NODE_ENV=production webpack --config webpack.config.dist.js --optimize-minimize",
    "build:playground": "rimraf build && cross-env NODE_ENV=production webpack --config webpack.config.prod.js --optimize-minimize && cp playground/index.prod.html build/index.html",
    "dist": "npm run build:lib && npm run build:dist",
    "lint": "eslint src test",
    "publish-to-gh-pages": "npm run build:playground && gh-pages --dist build/",
    "publish-to-npm": "npm run build:readme && npm run dist && npm publish",
    "start": "node devServer.js",
    "tdd": "cross-env NODE_ENV=test  mocha --compilers js:babel-register --watch --require ./test/setup-jsdom.js test/**/*_test.js",
    "test": " cross-env NODE_ENV=test   mocha --compilers js:babel-register --require ./test/setup-jsdom.js test/**/*_test.js"
  },
  "main": "lib/index.js",
  "files": [
    "dist",
    "lib"
  ],
  "engineStrict": false,
  "engines": {
    "npm": "^2.14.7",
    "node": ">=18"
  },
  "peerDependencies": {
    "react": "*"
  },
  "dependencies": {
    "jsonschema": "^1.4.1",
    "lodash.topath": "^4.5.2",
    "prop-types": "^15.8.1",
    "react-is": "^17.0.2",
    "setimmediate": "^1.0.5"
  },
  "devDependencies": {
    "atob": "^2.1.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^5.1.1",
    "codemirror": "^6.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "eslint": "^9.9.1",
    "eslint-plugin-react": "^7.35.0",
    "estraverse": "^4.2.0",
    "estraverse-fb": "^1.3.2",
    "express": "^4.19.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "gh-pages": "^6.1.1",
    "html": "1.0.0",
    "jsdom": "^25.0.0",
    "mocha": "^10.7.3",
    "react": "^18.3.1",
    "react-codemirror": "^1.0.0",
    "react-dom": "^18.3.1",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.6.0",
    "rimraf": "^6.0.1",
    "sinon": "^18.0.0",
    "style-loader": "^4.0.0",
    "toctoc": "^0.4.0",
    "webpack": "^5.94.0",
    "webpack-dev-middleware": "^7.4.2",
    "webpack-hot-middleware": "^2.26.1"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/department-of-veterans-affairs/react-jsonschema-form.git"
  },
  "author": "DSVA",
  "keywords": [
    "react",
    "form",
    "json-schema"
  ],
  "license": "Apache-2.0",
  "homepage": "https://github.com/department-of-veterans-affairs/react-jsonschema-form#readme"
}
